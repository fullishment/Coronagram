<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="infostep">

	<select id="getSteplist" parameterType="hashmap" resultType="hashmap">
		SELECT MGT_NO, AREA_NO, STEP_NO, CON
		FROM STEP_MGT
		WHERE DEL = 1
	</select>
	
</mapper>